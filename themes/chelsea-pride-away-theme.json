    "quickInput.foreground": "#FFFFFF",
    "pickerGroup.foreground": "#FFD700",
    "pickerGroup.border": "#1C2C70"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#A9A9A9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#1E90FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#DBAA11"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#87CEFA"
      }
    },
    {
      "name": "Numbers and Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant"
      ],
      "settings": {
        "foreground": "#FFA500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators and Punctuation",
      "scope": [
        "punctuation",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Readonly Variables",
      "scope": "variable.readonly",
      "settings": {
        "foreground": "#FF8C00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#B0C4DE"
      }
    }
  ],
  "semanticHighlighting": true
}